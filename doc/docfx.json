{
  "metadata": [
    {
      "src": [
        {
          "files": [
            "src/Confluent.Kafka/*.cs"
          ],
          "cwd": ".."
        }
      ],
      "dest": "api"
    }
  ],
  "build": {
    "content": [
      {
        "files": [
          "api/**.yml",
          "toc.yml",
          "*.md"
        ]
      }
    ],
    "globalMetadata": {
      "_appTitle": "Confluent.Kafka",
      "_disableContribution": true,
      "_disableFooter": true
    },
    "dest": "_site",
    "template": ["default", "template"]
  }
}
