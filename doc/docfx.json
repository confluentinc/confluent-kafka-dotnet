{
  "metadata": [
    {
      "src": [
        {
          "files": [
            "src/Confluent.Kafka/Confluent.Kafka.csproj",
            "src/Confluent.SchemaRegistry**/*.csproj"
          ],
          "src": ".." 
        }
      ],
      "dest": "api",
      "properties": {
              "TargetFramework": "net6.0"
      }
    }
  ],
  "build": {
    "content": [
      {
        "files": [
          "api/**.yml",
          "*.md"
        ]
      }
    ],
    "globalMetadata": {
      "_appTitle": "Confluent.Kafka",
      "_disableContribution": true,
      "_disableFooter": true
    },
    "dest": "_site",
    "template": ["default", "template"]
  }
}
