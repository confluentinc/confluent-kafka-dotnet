{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "Kafka": {
    "ProducerSettings": {
        "BootstrapServers": "<confluent cloud bootstrap servers>",
        "SaslMechanism": "plain",
        "SecurityProtocol": "sasl_ssl",
        "SaslUsername": "<confluent cloud key>",
        "SaslPassword": "<confluent cloud secret>"
    },
    "ConsumerSettings": {
        "BootstrapServers": "<confluent cloud bootstrap servers>",
        "GroupId": "web-example-group",
        "SaslMechanism": "plain",
        "SecurityProtocol": "sasl_ssl",
        "SaslUsername": "<confluent cloud key>",
        "SaslPassword": "<confluent cloud secret>"
    },
    "RequestTimeTopic": "request_times",
    "FrivolousTopic": "frivolous_topic"
  },
  "AllowedHosts": "*"
}
